{
   "version": "0.2.0",
   "configurations":
   [
      {
         "name": "GDB Debug ZenUnitLibraryTests",
         "type": "cppdbg",
         "request": "launch",
         "preLaunchTask": "Ninja Build Debug",
         "cwd": "${workspaceRoot}",
         "program": "Debug/ZenUnitLibraryTests/ZenUnitLibraryTests",
         "linux": { "MIMode": "gdb" }
      },
      {
         "name": "Debug Python",
         "type": "python",
         "python": "/usr/bin/python",
         "request": "launch",
         "program": "${file}",
         "args": [ "" ],
         "env": { "PYTHONPATH": ".." },
         "stopOnEntry": false,
         "console": "integratedTerminal",
         "cwd": "${fileDirname}"
      }
   ]
}
